// 2293 ë™ì „1

import sys
input = sys.stdin.readline

n, k = map(int, input().strip().split())
data = [int(input().strip()) for _ in range(n)]

d = [0] * (k+1)
d[0] = 1

for i in range(n):
    for j in range(data[i], k+1):
        if d[j-data[i]] >= 0:
            d[j] += d[j-data[i]]

print(d[k])
